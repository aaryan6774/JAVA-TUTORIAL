public class Main {
    public static void main(String[] args) {
        System.out.println(3+5);
        System.out.println(8*7);
        int num = 3;
        System.out.println(num);
        int num2 = 5;
        System.out.println(num2);
        int result = num + num2;
        System.out.println(result);
        }
    }
